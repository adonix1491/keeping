{
    "devCommand": "npx expo start --web",
    "buildCommand": "npx expo export -p web",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/check",
            "schedule": "*/10 * * * *"
        }
    ]
}