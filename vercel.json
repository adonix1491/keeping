{
    "devCommand": "npx expo start --web",
    "buildCommand": "npx expo export -p web",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/|.*\\..*).*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/check",
            "schedule": "0 16 * * *"
        }
    ]
}