{
    "devCommand": "npx expo start --web",
    "buildCommand": "npx expo export -p web",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/check",
            "schedule": "*/10 * * * *"
        }
    ]
}